var 
    a: Integer;
    b: Integer;
    c: Integer;
    d: Integer;
begin
    a := 2;
    b := 5;
    c := 64;
    d := a << b;
    write(d); //64
    write(#10);
    d := c >> b;
    write(d); //2
    write(#10);
    d := a << 5;
    write(d); //64
    write(#10);
    d := c >> 5;
    write(d); //2
    write(#10);
    d := a and b;
    write(d); //0
    write(#10);
    d := a or b;
    write(d); //7
    write(#10);
    d := a xor b;
    write(d); //7
    write(#10);
    d := not a;
    write(d); //-3
    write(#10);
end.